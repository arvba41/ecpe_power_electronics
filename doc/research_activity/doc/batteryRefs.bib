@inproceedings{hatzell_survey_2012,
	address = {Montreal, QC},
	title = {A survey of long-term health modeling, estimation, and control of {Lithium}-ion batteries: {Challenges} and opportunities},
	isbn = {978-1-4577-1096-4 978-1-4577-1095-7 978-1-4577-1094-0 978-1-4673-2102-0},
	shorttitle = {A survey of long-term health modeling, estimation, and control of {Lithium}-ion batteries},
	url = {http://ieeexplore.ieee.org/document/6315578/},
	doi = {10.1109/ACC.2012.6315578},
	abstract = {This paper reviews the literature on Lithium-ion battery characterization, control, and optimization. The paper examines the primary degradation processes in cycled cells, then proceeds to the challenges associated with controlling these processes. One key challenge is the multiplicity of complex phenomena contributing to battery aging, and the failure of most empirical battery health models to account for this multiplicity. This creates a need for identifying how batteries fail, and developing control-oriented fundamental models of their failure: a task that requires tight integration of experimental characterization techniques, system identiﬁcation, and electrochemistry-based battery modeling and simulation. The paper provides an overview of battery degradation mechanisms and reactions, and the various methods used to characterize these reactions. We also highlight the battery control strategies currently in use, and the potential for long-term battery health improvements using model-based degradation control.},
	language = {en},
	urldate = {2022-11-23},
	booktitle = {2012 {American} {Control} {Conference} ({ACC})},
	publisher = {IEEE},
	author = {Hatzell, K. B. and Sharma, A. and Fathy, H. K.},
	month = jun,
	year = {2012},
	pages = {584--591},
	file = {2012-SurveyLongTermACC.pdf:/Users/larer34/Nextcloud/Documents/ProjectDocs/BatteryModeling/Spectroscopy/2012-SurveyLongTermACC.pdf:application/pdf},
}

@article{li_electrochemistry-based_2014,
	title = {An electrochemistry-based impedance model for lithium-ion batteries},
	volume = {258},
	issn = {03787753},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0378775314002298},
	doi = {10.1016/j.jpowsour.2014.02.045},
	abstract = {Accurate models of lithium-ion batteries are important for analyzing and predicting battery dynamics and aging. This paper presents an electrochemistry-based impedance model for lithium-ion batteries to better understand the relationship between battery internal dynamics and external measurement. The proposed impedance model is a modiﬁed single particle model which balances between simplicity and accuracy. The model includes electrochemical impedance due to charge-transfer reaction, diffusion dynamics in the electrodes, effects of ion concentration, capacitance dispersion in the double layer, and anode insulating ﬁlm growth, etc. The impedance tests for model validation were performed on two lithium-ion cells at ambient temperature and at different SOC levels. A particle swarm optimization method is employed to identify model parameters. The model accuracy under different conditions is compared with that of conventional Randles model and the parameter variations at different stage of the aging process are studied.},
	language = {en},
	urldate = {2022-11-23},
	journal = {Journal of Power Sources},
	author = {Li, Shengbo Eben and Wang, Baojin and Peng, Huei and Hu, Xiaosong},
	month = jul,
	year = {2014},
	pages = {9--18},
	file = {2014-An electrochemistry-based impedance model for lithium-ion batteries.pdf:/Users/larer34/Nextcloud/Documents/ProjectDocs/BatteryModeling/Spectroscopy/2014-An electrochemistry-based impedance model for lithium-ion batteries.pdf:application/pdf},
}

@inproceedings{thanh-tuan_nguyen_development_2014,
