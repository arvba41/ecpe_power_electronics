@article{chen_fast_2021,
	title = {A {Fast} {Charging}–{Cooling} {Coupled} {Scheduling} {Method} for a {Liquid} {Cooling}-{Based} {Thermal} {Management} {System} for {Lithium}-{Ion} {Batteries}},
